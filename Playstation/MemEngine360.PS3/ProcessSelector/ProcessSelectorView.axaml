<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ps3="clr-namespace:MemEngine360.PS3"
             xmlns:processSelector="clr-namespace:MemEngine360.PS3.ProcessSelector"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MemEngine360.PS3.ProcessSelector.ProcessSelectorView">
    <StackPanel>
        <TextBlock Padding="4,2" Text="Select a process"/>
        <Separator/>
        <processSelector:ProcessListBox x:Name="PART_ProcessListBox"/>
    </StackPanel>
</UserControl>
