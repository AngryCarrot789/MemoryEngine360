<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MemEngine360.BaseFrontEnd.Services.Connectivity.ConsoleTypeToolTip">
    <StackPanel>
        <TextBlock x:Name="PART_Header" FontSize="20" Padding="4,2"/>
        <Separator/>
        <TextBox x:Name="PART_Id" FontSize="14" Padding="4,2" IsReadOnly="True" BorderThickness="0" Background="Transparent"/>
        <TextBlock x:Name="PART_Description" Padding="4,2" Opacity="0.7"/>
    </StackPanel>
</UserControl>
