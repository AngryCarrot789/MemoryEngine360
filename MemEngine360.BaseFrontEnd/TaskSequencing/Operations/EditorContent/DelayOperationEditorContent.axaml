<editorContent:BaseOperationEditorContent xmlns="https://github.com/avaloniaui"
                               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                               xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                               xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                               xmlns:editorContent="clr-namespace:MemEngine360.BaseFrontEnd.TaskSequencing.Operations.EditorContent"
                               xmlns:tt="clr-namespace:PFXToolKitUI.Avalonia.ToolTips;assembly=PFXToolKitUI.Avalonia"
                               mc:Ignorable="d" d:DesignHeight="200"
                               x:Class="MemEngine360.BaseFrontEnd.TaskSequencing.Operations.EditorContent.DelayOperationEditorContent"
                               Width="100">
    <StackPanel Margin="2">
        <TextBlock Padding="1,2" Text="Delay for"/>
        <TextBox x:Name="PART_DelayTextBox" Padding="2,2" VerticalContentAlignment="Center"
                 tt:ToolTipEx.Tip="Delay time. Note, less than 20 milliseconds will use high-precision timing, which increases CPU usage"
                 Background="{DynamicResource ABrush.Tone1.Background.Static}"/>
    </StackPanel>
</editorContent:BaseOperationEditorContent>