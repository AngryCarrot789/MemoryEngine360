<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Make sure to update any inheritance in MemoryEngineBrushLoader too! Otherwise the app won't look the same at runtime comapred to design time -->
    <!-- And obviously, if adding anything new in here, add it to MemoryEngineBrushLoader as well -->
    <SolidColorBrush x:Key="ABrush.MemEngine.MainView.Background"                                       Color="{StaticResource ABrush.Tone4.Background.Static.Color}"/>
