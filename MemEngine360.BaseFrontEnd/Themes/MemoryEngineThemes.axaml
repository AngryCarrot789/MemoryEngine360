    <SolidColorBrush x:Key="ABrush.MemEngine.MainView.AdditionOptions.TextBox.Foreground"               Color="{StaticResource ABrush.Foreground.Static.Color}"/>
    <SolidColorBrush x:Key="ABrush.MemEngine.MainView.AdditionOptions.Button.Background"                Color="{StaticResource ABrush.Tone7.Background.Static.Color}"/>
    <SolidColorBrush x:Key="ABrush.MemEngine.MainView.AdditionOptions.Button.Border"                    Color="{StaticResource ABrush.Tone7.Border.Static.Color}"/>
