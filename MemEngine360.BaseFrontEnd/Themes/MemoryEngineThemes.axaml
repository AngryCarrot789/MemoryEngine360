    <SolidColorBrush x:Key="ABrush.MemEngine.MainView.ScanOptions.TextBox.Foreground"                   Color="{StaticResource ABrush.Foreground.Static.Color}"/>
    <SolidColorBrush x:Key="ABrush.MemEngine.MainView.ScanOptions.ScanTypeComboBox.Background"          Color="{StaticResource ABrush.Tone2.Background.Static.Color}"/>
    <SolidColorBrush x:Key="ABrush.MemEngine.MainView.ScanOptions.ScanTypeComboBox.Border"              Color="{StaticResource ABrush.Tone6.Border.Static.Color}"/>
